{
  "manifest_version": 3,
  "name": "MARFTpro -Premio Web Integration",
  "version": "1.0",
  "description": "Integração entre MARFTpro e o textillab premios",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "http://192.168.1.150/MARFTpro/App/ProductionReadNew.aspx*",
    "http://192.168.1.150:5000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://192.168.1.150/MARFTpro/App/ProductionReadNew.aspx*",
        "http://192.168.1.150/MARFTpro/App/ProductionReadNew.aspx?*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["images/icon.gif", "overlay.html"],
    "matches": ["http://192.168.1.150/*"]
  }]
}